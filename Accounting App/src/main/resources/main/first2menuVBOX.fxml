<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<VBox xmlns:fx="http://javafx.com/fxml">
        <HBox>
                <MenuButton id="menuButton0" alignment="BOTTOM_CENTER"
                            contentDisplay="TOP" graphicTextGap="0.0"
                            mnemonicParsing="false" prefHeight="70.0"
                            stylesheets="@styles/Styles.css"
                            text="القائمة&#10; الاولي " textAlignment="CENTER"
                            textOverrun="CLIP">
                    <graphic>
                        <ImageView fitHeight="16" fitWidth="16"
                                   preserveRatio="true">
                                <Image url="@images/gears.png">
                                </Image>
                        </ImageView>
                    </graphic>
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1"/>
                        <MenuItem mnemonicParsing="false" text="Action 2"/>
                    </items>
                    <font>
                        <Font name="Dubai Regular" size="12.0"/>
                    </font>
                </MenuButton>
                <MenuButton id="menuButton0" fx:id="menuButton1"
                            alignment="BOTTOM_CENTER" contentDisplay="TOP"
                            graphicTextGap="0.0" mnemonicParsing="false"
                            prefHeight="70.0" stylesheets="@styles/Styles.css"
                            text="الارصدة&#10; الافتتاحية"
                            textAlignment="CENTER" textOverrun="CLIP">
                    <graphic>
                        <ImageView fitHeight="16" fitWidth="16"
                                   preserveRatio="true">
                                <Image url="@images/safe.png">
                                </Image>
                        </ImageView>
                    </graphic>
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1"/>
                        <MenuItem mnemonicParsing="false" text="Action 2"/>
                    </items>
                    <font>
                        <Font name="Dubai Regular" size="12.0"/>
                    </font>
                </MenuButton>
        </HBox>
        <Separator maxHeight="0.0" minHeight="0.0" minWidth="0.0"
                   prefHeight="0.0"
                   style="-fx-border-color: linear-gradient(to left, #0022ff, #00ff00); -fx-border-color: linear-gradient(to right, #0022ff, #00ff00); -fx-max-height: 0;"/>
        <Label alignment="TOP_LEFT" contentDisplay="CENTER"
               text="البيانات الاساسية" textAlignment="CENTER"
               textOverrun="CLIP" translateX="26.0">
            <padding>
                <Insets bottom="3.0"/>
            </padding>
        </Label>
</VBox>
